<%= form_with url: search_users_path, method: :get, local: true do |f| %>
  <%= f.label :name, "ユーザー名検索", class: "label-" %>
  <div class="form-inline">
    <%= f.text_field :name, class: "form-control ", placeholder: "キーワードを入力…", value: params[:name] %>
    <%= f.submit "検索", class: "btn btn-primary" %>
  </div>
<% end %>